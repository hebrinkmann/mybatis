<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="de.henningBrinkmann.mybatisSample.mapper.DescriptionMapper">
	<insert id="insertDescription" parameterType="de.henningBrinkmann.mybatisSample.xmltv.Description">
		INSERT INTO `mybatissample`.`description`
		(`value`,
		`lang`)
		VALUES
		(#{value},
		#{lang});
	</insert>
</mapper>
